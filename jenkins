package com.surge.colling;

public class Sample1
{
	int rollno;
	String name;
	static String college="sgems";
	static void change()
	{
		college="stanns";
	}
	public Sample1(int rollno, String name) {
		super();
		this.rollno = rollno;
		this.name = name;
	}
	void display()
	{
		System.out.println(rollno+" "+name+" "+college);
		
	}
	
	
	
}

